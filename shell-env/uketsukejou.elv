set paths = [
  $E:HOME'/.cargo/bin'
  $E:HOME'/.local/bin'
  $E:HOME'/.fzf/bin'
  '/usr/local/go/bin'
  '/usr/local/ssl/bin'
  '/usr/local/bin'
  '/usr/bin'
  '/bin'
  '/sbin'
]

set E:LANG = 'ja_JP.UTF-8'
set E:EDITOR = 'nvim'
set E:VISUAL = 'nvim'
set E:JAVA_HOME = '/usr/lib/jvm/'(archlinux-java get)
set E:DOTNET_ROOT = '/usr/share/dotnet'
